    "ts-jest": "29.2.5",
    "ts-loader": "9.5.1",
    "typescript": "5.6.2",
    "typescript-eslint": "8.6.0",
    "wait-on": "8.0.1",
    "webpack": "5.94.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "5.1.0",
